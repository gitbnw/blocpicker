 <% if stocks.empty?  %>
   <h4 class="empty_head">Portfolio is empty.  Please add some stocks.</h4>
  <% else %>
  <div class="row" id="list_head">
   <div class="col-md-1">Symbol</div>
   <div class="col-md-1">Price</div>
   <div class="col-md-1">Change</div>
   <div class="col-md-1">Percent</div>
   <div class="col-md-2">Volume</div>
   <div class="col-md-2">Avg Dly Vol</div>
   <div class="col-md-1 tag-tooltip dotted" data-toggle="tooltip" data-placement="top" title="Tooltip on top">Bounce</div>
   <div class="col-md-2">Updated_At</div>
  </div>
  
  <%= render stocks %>

 <% end %>